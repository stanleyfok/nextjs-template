{
  "name": "nextjs-template",
  "version": "1.0.0",
  "description": "Nextjs project template",
  "repository": {
    "type": "git",
    "url": "git://github.com/stanleyfok/nextjs-template.git"
  },
  "scripts": {
    "debug": "npm run cc; node --inspect=9230 server.js",
    "dev": "npm run cc; node server.js",
    "build": "npm run cc; next build; npm run copy-static; npm run replace-build-id;",
    "prestart": "npm run build",
    "start": "node server.js",
    "copy-static": "cp -r .next/static .",
    "replace-build-id": "cat package.json | jq -r '.version' | md5sum | awk '{print $1}' > .next/BUILD_ID",
    "analyze": "ANALYZE=1 next build",
    "cc": "rm -rf .next static"
  },
  "author": "Stanley Fok <stanleyfok@gmail.com> (https://github.com/stanleyfok)",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^7.2.4",
    "axios": "^0.18.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-define": "^1.3.0",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "bootstrap": "^4.1.1",
    "compression": "^1.7.2",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.11",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "extracted-loader": "^1.0.7",
    "file-loader": "^1.1.11",
    "glob": "^7.1.2",
    "i18next": "^10.2.2",
    "i18next-browser-languagedetector": "^2.1.0",
    "i18next-express-middleware": "^1.0.9",
    "i18next-node-fs-backend": "^1.0.0",
    "i18next-xhr-backend": "^1.5.1",
    "md5": "^2.2.1",
    "next": "latest",
    "next-redux-wrapper": "^1.3.5",
    "next-routes": "^1.4.2",
    "node-sass": "^4.7.2",
    "nprogress": "^0.2.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^2.0.10",
    "prop-types": "^15.6.0",
    "raw-loader": "^0.5.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-i18next": "^7.3.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^6.0.6",
    "serialize-error": "^2.1.0",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.1.3",
    "url-join": "^3.0.0",
    "url-loader": "^1.0.1",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-react": "^7.5.1",
    "redux-mock-store": "^1.5.1",
    "sass-lint": "^1.12.1"
  }
}
